<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="mrp_production_form_view_tracability_mgt_jsi" model="ir.ui.view">
            <field name="name">mrp.production.form.view.tracability.mgt.jsi</field>
            <field name="model">mrp.production</field>
            <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='bom_id']" position="after">
                    <label for="mh_tracking_no"/>
                    <div>
                        <field name="mh_tracking_no" class="oe_inline"/>
                        <button name="button_transfer_details" type="object" string="Transfer to Picking" class="oe_link oe_edit_only"/>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>