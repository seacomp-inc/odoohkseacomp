<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_order_line_supply_chain_report_tree_jsi" model="ir.ui.view">
            <field name="name">view.order.line.supply.chain.report.tree.jsi</field>
            <field name="model">sale.order.line</field>
            <field name="inherit_id" ref="sale.view_order_line_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="inside">
                    <field name="cost_total"/>
                    <field name="profit_loss"/>
                    <field name="margin_new"/>
                    <button name="open_supply_chain_report" type="object" string="Supply Chain" class="btn"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
